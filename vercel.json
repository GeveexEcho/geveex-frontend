{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/api/claim",
      "destination": "/api/claim.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
